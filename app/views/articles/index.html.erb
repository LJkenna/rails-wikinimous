<div class=" text-center align-items "  id="container">
<div class="row">
<% @articles.each do |article| %>
<h2><%=link_to article.title, article_path(article), class:"text-decoration-none text-info"%>
<%= link_to article_path(article), data: {turbo_method: :delete, turbo_confirm: "Are you sure want to delete this?"}  do  %>
<i class="fa-solid fa-trash"></i>
<% end %></h2>
<h3><%=article.content%></h3>
<% end  %>
